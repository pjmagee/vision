{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "defaultDestination": "wwwroot/@lib/",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/@lib/jquery"
    },
    {
      "library": "datatables@1.10.19",
      "destination": "wwwroot/@lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js",
        "js/dataTables.bootstrap4.min.js",
        "css/dataTables.bootstrap4.min.css"
      ]
    },
    {
      "library": "Chart.js@2.7.3",
      "destination": "wwwroot/@lib/Chart.js/",
      "files": [ "Chart.min.js" ]
    },
    {
      "library": "jquery-easing@1.4.1",
      "destination": "wwwroot/@lib/jquery-easing/",
      "files": [ "jquery.easing.min.js" ]
    },
    {
      "library": "font-awesome@5.7.2",
      "destination": "wwwroot/@lib/font-awesome/",
      "files": [ "css/all.min.css" ]
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap@4.3.1",
      "destination": "wwwroot/@lib/bootstrap/"
    }
  ]
}